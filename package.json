{
    "name": "swr-cache-proxy",
    "license": "BSD-2-Clause",
    "scripts": {
        "build": "$npm_execpath run clean && tsc -p tsconfig.build.json",
        "clean": "rimraf dist",
        "dev": "tsc --watch --preserveWatchOutput -p tsconfig.build.json",
        "lint": "run-p lint:eslint lint:tsc",
        "lint:eslint": "eslint --max-warnings 0 src/ package.json",
        "lint:tsc": "tsc --noEmit",
        "postinstall": "husky install",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "commander": "^11.0.0",
        "express": "^4.18.2",
        "fs-extra": "^11.1.1",
        "typescript": "^5.2.2"
    },
    "devDependencies": {
        "@comet/eslint-config": "^4.4.1",
        "@types/express": "^4.17.17",
        "@types/fs-extra": "^11.0.1",
        "@types/jest": "^29.5.4",
        "@types/supertest": "^2.0.12",
        "eslint": "^8.48.0",
        "husky": "^8.0.3",
        "jest": "^29.6.4",
        "jest-junit": "^16.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.0",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1"
    }
}
